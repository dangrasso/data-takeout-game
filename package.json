{
  "name": "@dangrasso/data-takeout-game",
  "private": true,
  "version": "0.0.1",
  "description": "Data Takeout: the game",
  "main": "dist/mini-game.js",
  "module": "dist/mini-game.js",
  "type": "module",
  "scripts": {
    "build": "tsc && rollup -c",
    "build:watch": "tsc-watch --onSuccess \"rollup -c\"",
    "clean": "rimraf dist/**/*.{d.ts,d.ts.map,js,js.map} rimraf test/**/*.{d.ts,d.ts.map,js,js.map} && npm run docs:clean",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "docs": "npm run docs:init && npm run docs:html && npm run docs:js && npm run docs:img",
    "docs:init": "mkdir -p docs && touch docs/.nojekyll",
    "docs:html": "cp index.html docs",
    "docs:js": "mkdir -p docs/dist && cp dist/*.bundled.js docs/dist/",
    "docs:img": "cp -R img docs && rm -rf docs/img/_*",
    "docs:clean": "rimraf docs",
    "serve": "wds --watch"
  },
  "keywords": [
    "game",
    "takeout",
    "privacy",
    "typescript"
  ],
  "author": "Daniele Grasso",
  "license": "BSD-3-Clause",
  "dependencies": {
    "lit": "^2.0.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.5.3",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@web/dev-server": "^0.1.22",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/dev-server-rollup": "^0.3.9",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "eslint": "^7.32.0",
    "lit-analyzer": "^1.1.10",
    "mocha": "^9.1.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.2",
    "rollup-plugin-summary": "^1.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.3.5"
  },
  "customElements": "custom-elements.json"
}
